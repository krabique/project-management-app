<div class="container">
  <div class="panel panel-default">
    <div class="panel-body">
      
      <% if user_signed_in? %>
        <div class="row">
          
          <div class="col-sm-6">
            <%= render "home/all_projects" %>
          </div>
        
          <div class="col-sm-6">
            <%= render "home/my_projects" %>
          </div>
          
        </div>
      <% else %>
        <%= render "home/all_projects" %>
      <% end %>
      
    </div>
  </div>
</div> 