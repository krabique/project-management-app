<% p "-----------start home projects view-------------------------------------" %>
<div class="container">
  <div class="panel panel-default">
    <div class="panel-body">
      <% if user_signed_in? %>
        <div class="row">
          
          <div class="col-sm-6">
            <% p "------start ----render home/all_projets --------" %>
            <%= render "home/all_projects" %>
            <% p "------finish----render home/all_projets --------" %>
            <br>
            
            <% p "-----start-------can can can check ------------" %>
            <%= link_to (raw '<i class="fa fa-plus-circle" aria-hidden="true">
              </i> Create new project'), new_project_path if 
              (can? :manage, Project) %>
            <% p "-----finish-------can can can check ------------" %>
          </div>
        
          <div class="col-sm-6">
            <% p "------start ----render home/my_projects ----123----" %>
            <%= render "home/my_projects" %>
            <% p "------finish ----render home/my_projects -----123---" %>
          </div>
          
        </div>
      <% else %>
        <%= render "home/all_projects" %>
      <% end %>
      
    </div>
  </div>
</div> 
<% p "-----------finish home projects view---------------------------------------------" %>
